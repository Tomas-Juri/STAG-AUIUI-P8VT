@model Application.Api.Models.AccountViewModel
@{
    ViewData["Title"] = "Account";
}

<div class="row justify-content-center">
    <div class="col-sm-12 col-md-10 col-lg-7 col-xl-5">
        <h1>Account</h1>
        <form >
            <div>
                <label asp-for="Email" class="form-label mt-4">Email address</label>
                <input asp-for="Email" class="form-control" disabled="">
            </div>
            <div>
                <label asp-for="Name" class="form-label mt-4">Name</label>
                <input asp-for="Name" class="form-control" disabled="">
            </div>
        </form>
    </div>
</div>